#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define nl "\n"
#define endl "\n"
#define fast ios_base::sync_with_stdio(false); cin.tie(NULL);






int main ()
{
    //fast;
    srand((unsigned int)time(NULL));  //what is this??
    string a="";
    for (int  i = 0; i < 8; i++)
    {
        char c = rand()%26+65;
        char d = rand()%26+97;
      //  cout<<c<<nl<<d<<nl;
        int x = rand()%2;
      //  cout<<x;
        if(x==0) a=c;
        else a+=d;
    }

    cout<<a;
    






    return 0;
}

